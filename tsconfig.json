{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "baseUrl": "./",
    "paths": {
      "#apis/*": [
        "./apis/*"
      ],
      "#assets/*": [
        "./assets/*"
      ],
      "#commons/*": [
        "./commons/*"
      ],
      "#components/*": [
        "./components/*"
      ],
      "#contexts/*": [
        "./contexts/*"
      ],
      "#hooks/*": [
        "./hooks/*"
      ],
      "#interfaces/*": [
        "./interfaces/*"
      ],
      "#pages/*": [
        "./pages/*"
      ],
      "#public/*": [
        "./public/*"
      ],
      "#stores/*": [
        "./stores/*"
      ],
      "#styles/*": [
        "./styles/*"
      ],
      "#tests/*": [
        "./tests/*"
      ],
      "#types/*": [
        "./types/*"
      ],
      "#utils/*": [
        "./utils/*"
      ]
    },
    "rootDir": ".",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
